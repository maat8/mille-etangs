    <url>
        <loc>{{ url }}{{ path('randonnees_type', {'type':'cheval'}) }}</loc>
        <changefreq>weekly</changefreq>
    </url>
    <url>
        <loc>{{ url }}{{ path('hebergement') }}</loc>
        <changefreq>monthly</changefreq>
    </url>
    <url>
        <loc>{{ url }}{{ path('restauration') }}</loc>
        <changefreq>monthly</changefreq>
    </url>
    <url>
        <loc>{{ url }}{{ path('hebergement') }}</loc>
        <changefreq>monthly</changefreq>
    </url>
    <url>
        <loc>{{ url }}{{ path('contact') }}</loc>
        <changefreq>monthly</changefreq>
    </url>
    {% for rando in randonneesVtt %}
	    <url>
	        <loc>{{ url }}{{ path('randonnee', {'slug':rando.getSlug()}) }}</loc>
	        <changefreq>monthly</changefreq>
	    </url>
    {% endfor %}
</urlset>