<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
		xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
{% set url = "http://www.randos-mille-etangs.fr" %}
	<url>
        <loc>{{ url }}</loc>
        <changefreq>daily</changefreq>
    </url>
    <url>
        <loc>{{ url }}{{ path('milleEtangs') }}</loc>
        <changefreq>monthly</changefreq>
    </url>
	<url>
        <loc>{{ url }}{{ path('itinearies_type', {'type':'bike'}) }}</loc>
        <changefreq>weekly</changefreq>
    </url>
    <url>
        <loc>{{ url }}{{ path('itinearies_type', {'type':'hike'}) }}</loc>
        <changefreq>weekly</changefreq>
    </url>
    <url>
        <loc>{{ url }}{{ path('accomodations') }}</loc>
        <changefreq>monthly</changefreq>
    </url>
    <url>
        <loc>{{ url }}{{ path('restaurants') }}</loc>
        <changefreq>monthly</changefreq>
    </url>
    <url>
        <loc>{{ url }}{{ path('contact') }}</loc>
        <changefreq>monthly</changefreq>
    </url>
    {% for itineary in itineariesBike %}
	    <url>
	        <loc>{{ url }}{{ path('itineary', {'slug':itineary.getSlug()}) }}</loc>
	        <changefreq>monthly</changefreq>
	    </url>
    {% endfor %}
    {% for itineary in itineariesHike %}
        <url>
            <loc>{{ url }}{{ path('itineary', {'slug':itineary.getSlug()}) }}</loc>
            <changefreq>monthly</changefreq>
        </url>
    {% endfor %}
</urlset>